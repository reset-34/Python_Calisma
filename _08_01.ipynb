    "n = int (input (\"n değerini giriniz (Pozitif tam sayı olmalı)\"))\n",
    "\n",
    "if n<0:\n",
    "    print (\"Hata: Negatif Sayılar İçin Faktöriyel Hesaplanmaz.\")\n",
    "else:\n",
    "    faktoriyel = 1\n",
    "    for i in range (1, n+1):\n",
    "        faktoriyel*= i\n",
    "    print (f\"{n}!={faktoriyel}\")\n"
   ]
  }
